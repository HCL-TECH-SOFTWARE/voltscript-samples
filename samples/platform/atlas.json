{
    "name": "platform-samples",
    "description": "HCL VoltScript Platform Samples",
    "version": "1.0.0",
    "authors": [
        "Paul Withers"
    ],
    "license": "Apache 2.0",
    "sourceDir": "src",
    "testDir": "test",
    "libsDir": "libs",
    "vsesDir": "vses",
    "mainScripts": [
        "main.vss"
    ],
    "unitTestScripts": [
        "unittests.vss"
    ],
    "repositories": [
        {
            "id": "volt-mx-marketplace",
            "type": "marketplace",
            "url": "https://community.demo-hclvoltmx.com/marketplace"
        },
        {
            "id": "hcl-github",
            "type": "github",
            "url": "https://api.github.com/repos/HCL-TECH-SOFTWARE"
        }
    ],
    "dependencies": [
        {
            "library": "voltscript-testing",
            "version": "1.0.1",
            "module": "VoltScriptTesting.vss",
            "repository": "hcl-github"
        }
    ],
    "vseDependencies": {
        "OSUtilsVSE": {
            "library": "OSUtils VoltScript Extension",
            "version": "1.0.2",
            "module": "osutilsvse",
            "repository": "volt-mx-marketplace"
        },
        "ZipVSE": {
            "library": "ZipVSE VoltScript Extension",
            "version": "1.0.2",
            "module": "zipvse",
            "repository": "volt-mx-marketplace"
        },
        "StreamVSE": {
            "library": "StreamVSE VoltScript Extension",
            "version": "1.0.2",
            "module": "streamvse",
            "repository": "volt-mx-marketplace"
        }
    }
