meta {
  name: getView
  type: http
  seq: 5
}

get {
  url: {{serverUrl}}/lists/Customers?dataSource={{scope}}
  body: none
  auth: inherit
}

params:query {
  dataSource: {{scope}}
}

headers {
  accept: application/json
}

script:post-response {
  if (res.body.length > 0) {
    const unids = [];
    unids.push(res.body[0]["@unid"]);
    unids.push(res.body[1]["@unid"]);
    unids.push(res.body[2]["@unid"]);
    bru.setEnvVar("unids",unids);
  }
}
